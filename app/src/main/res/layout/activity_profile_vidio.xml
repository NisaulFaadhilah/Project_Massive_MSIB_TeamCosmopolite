                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_10pxh"
                    android:text="@string/msg_lorem_ipsum_dol"
                    style="@style/txtPoppinsregular12" tools:text="@string/msg_lorem_ipsum_dol"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUmur"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
