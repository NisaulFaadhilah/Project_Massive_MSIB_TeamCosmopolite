                    android:layout_gravity="start"
                    android:layout_height="@dimen/_151pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_2_1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_2_1"/>
                <ImageView android:id="@+id/imageArrowOne"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_2pxh"
                    android:layout_marginBottom="@dimen/_38pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_38pxv"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow1"/>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackrectangletwentynine"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_133pxv"
                android:layout_marginBottom="@dimen/_55pxv"
                android:layout_marginEnd="@dimen/_73pxh"
                android:layout_marginStart="@dimen/_73pxh"
                android:layout_marginTop="@dimen/_55pxv"
                android:layout_width="@dimen/_214pxh" style="@style/groupStylewhite_1">
