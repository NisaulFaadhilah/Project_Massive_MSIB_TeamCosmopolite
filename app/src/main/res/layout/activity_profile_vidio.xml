                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_7_tahun"
                    style="@style/txtPoppinsregular12" tools:text="@string/lbl_7_tahun"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJenisVidio"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_jenis_vidio"
                    style="@style/txtPoppinsbold12" tools:text="@string/lbl_jenis_vidio"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAnimasi"
