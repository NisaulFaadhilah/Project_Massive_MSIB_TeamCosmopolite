                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_31pxv"
                        android:layout_marginEnd="@dimen/_49pxh"
                        android:layout_marginStart="@dimen/_49pxh"
                        android:layout_marginTop="@dimen/_31pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bookmarkadd"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bookmarkadd"/>
                </FrameLayout>
            </HorizontalScrollView>
