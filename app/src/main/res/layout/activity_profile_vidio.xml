                        android:layout_height="@dimen/_153pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_8_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_8_1"/>
