                    android:layout_gravity="center"
                    android:layout_height="@dimen/_117pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_197pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle29}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle29"/>
            </FrameLayout>
            <ImageView android:id="@+id/imageStatusBar"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_15pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_statusbar_4"
                tools:ignore="ContentDescription" tools:src="@drawable/img_statusbar_4"/>
            <HorizontalScrollView android:fillViewport="true"
                android:id="@+id/horizontalScrollHorizontalscrolleight"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="match_parent" android:scrollbars="none">
